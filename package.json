{
  "name": "cypress-fail-fast",
  "version": "2.3.2",
  "description": "Skip the rest of Cypress tests on first failure",
  "keywords": [
    "cypress",
    "cypress-plugin",
    "fail-fast",
    "failure",
    "skip",
    "cancel",
    "tests",
    "testing-tools",
    "testing",
    "parallel"
  ],
  "author": "Javier Brea",
  "license": "MIT",
  "repository": "https://github.com/javierbrea/cypress-fail-fast",
  "files": [
    "src",
    "index.d.ts",
    "plugin.js",
    "plugin.d.ts"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "lint": "eslint src test *.js test-e2e/*.js test-e2e/commands/**/*.js test-e2e/cypress-src/**/*.js test-e2e/test/**/*.js",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:unit": "npm run test",
    "test:e2e:install": "cd test-e2e && npm i",
    "test:e2e:run": "cd test-e2e && npm run test:ci",
    "test:e2e:debug": "cross-env npm run test:e2e:install && DEBUG=true npm run test:e2e:run",
    "test:e2e:ci": "npm run test:e2e:install && npm run test:e2e:run",
    "test:ci": "npm run test && npm run test:mutation && npm run test:e2e:ci",
    "test:mutation": "stryker run"
  },
  "peerDependencies": {
    "cypress": "5.x || 6.x || 7.x"
  },
  "devDependencies": {
    "@stryker-mutator/core": "4.6.0",
    "@stryker-mutator/jest-runner": "4.6.0",
    "babel-eslint": "10.1.0",
    "cross-env": "7.0.3",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "11.0.0",
    "prettier": "2.2.1",
    "sinon": "10.0.0"
  },
  "lint-staged": {
    "*.js": "eslint",
    "src/**/*.js": "eslint",
    "test/**/*.js": "eslint",
    "test-e2e/*.js": "eslint",
    "test-e2e/commands/**/*.js": "eslint",
    "test-e2e/cypress-src/**/*.js": "eslint",
    "test-e2e/test/**/*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": "10.x || 12.x || 14.x || 15.x || 16.x"
  }
}
